            <Button fx:id="submit" mnemonicParsing="false" onAction="#submitHandle" prefHeight="44.0" prefWidth="83.0" text="Créer" BorderPane.alignment="CENTER">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <BorderPane.margin>
                  <Insets bottom="10.0" />
               </BorderPane.margin>
            </Button>
         </bottom>
         <top>
            <HBox prefHeight="33.0" prefWidth="257.0">
               <BorderPane.margin>
                  <Insets top="10.0" />
               </BorderPane.margin>
               <children>
                  <Button fx:id="prev" mnemonicParsing="false" text="Précédent" onAction="#prevHandle" >
                     <HBox.margin>
                        <Insets left="10.0" top="3.0" />
                     </HBox.margin></Button>
               </children>
           </HBox>
         </top>
         <center>
            <GridPane BorderPane.alignment="CENTER">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="271.0" minWidth="10.0" prefWidth="196.0" />
