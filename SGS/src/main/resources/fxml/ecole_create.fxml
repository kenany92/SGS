                        <Label alignment="CENTER" prefHeight="24.0" prefWidth="36.0" text="/">
                           <HBox.margin>
                              <Insets left="3.0" right="3.0" top="8.0" />
                           </HBox.margin>
                        </Label>
                        <DatePicker fx:id="ase_f" prefHeight="26.0" prefWidth="147.0">
                           <HBox.margin>
                              <Insets top="7.0" />
                           </HBox.margin>
                        </DatePicker>
                     </children>
                  </HBox>
                  <Label text="ANNEE SCOLAIRE" GridPane.rowIndex="3" />
                  <ImageView fx:id="logoviewer_f" fitHeight="73.0" fitWidth="101.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                  <Button fx:id="logo_f" mnemonicParsing="false" text="LOGO" GridPane.rowIndex="4" />
               </children>
            </GridPane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
