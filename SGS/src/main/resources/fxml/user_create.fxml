<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.ecole.sgs.ui.user.UserCreateController">
    <stylesheets>
        <URL value="@/styles/Styles.css" />
    </stylesheets>
   <children>
      <BorderPane prefHeight="400.0" prefWidth="601.0">
         <top>
            <Label alignment="CENTER" prefHeight="45.0" prefWidth="460.0" text="Créer un compte administrateur" BorderPane.alignment="CENTER">
               <font>
                  <Font name="System Bold" size="19.0" />
               </font>
            </Label>
         </top>
         <right>
            <Label fx:id="droit" BorderPane.alignment="CENTER" />
         </right>
         <left>
            <Label fx:id="gauche" BorderPane.alignment="CENTER" />
         </left>
         <bottom>
            <Button fx:id="submit" mnemonicParsing="false" prefHeight="54.0" prefWidth="113.0" text="Créer" BorderPane.alignment="CENTER" onAction="#submitHandle"  >
               <font>
                  <Font name="System Bold" size="18.0" />
               </font></Button>
         </bottom>
         <center>
            <GridPane prefHeight="301.0" prefWidth="520.0" BorderPane.alignment="CENTER">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="294.0" minWidth="10.0" prefWidth="195.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="425.0" minWidth="10.0" prefWidth="406.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints maxHeight="64.0" minHeight="10.0" prefHeight="62.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="94.0" minHeight="10.0" prefHeight="57.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="189.0" minHeight="8.0" prefHeight="60.0" vgrow="SOMETIMES" />
                  <RowConstraints maxHeight="189.0" minHeight="10.0" prefHeight="123.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label prefHeight="43.0" prefWidth="133.0" text="Login" GridPane.rowIndex="1">
                     <GridPane.margin>
                        <Insets left="10.0" />
                     </GridPane.margin></Label>
                  <Label prefHeight="46.0" prefWidth="116.0" text="Mot de passe" GridPane.rowIndex="2">
                     <GridPane.margin>
                        <Insets left="10.0" />
                     </GridPane.margin>
                  </Label>
                  <PasswordField fx:id="psw_f" prefHeight="46.0" prefWidth="300.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
                     <GridPane.margin>
                        <Insets left="10.0" right="10.0" />
                     </GridPane.margin>
                     <font>
                        <Font size="15.0" />
                     </font>
                  </PasswordField>
                  <TextField fx:id="login_f" prefHeight="47.0" prefWidth="406.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                     <GridPane.margin>
                        <Insets left="10.0" right="10.0" />
                     </GridPane.margin>
                     <font>
                        <Font size="15.0" />
                     </font>
                  </TextField>
               </children>
            </GridPane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
